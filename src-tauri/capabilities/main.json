{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile-capability",
  "windows": [
    "main"
  ],
  "platforms": [
    "iOS",
    "android"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.tauri.app"
        }
      ],
      "deny": [
        {
          "url": "https://private.tauri.app"
        }
      ]
    },
    "http:default",
    "store:default"
  ]
}